{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.26",
        "darkaonline/swagger-lume": "5.5.*",
        "dingo/api": "2.0.0-alpha1",
        "doctrine/annotations": "1.4.0",
        "doctrine/dbal": "^2.10",
        "illuminate/mail": "5.5.*",
        "illuminate/redis": "5.*",
        "laravel/lumen-framework": "5.5.*",
        "symfony/var-dumper": "^4.2",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "../inc/lib/coderdbconf.php",
            "../inc/class/class_password.php",
            "../inc/class/class_firebase.php",
            "../inc/class/class_timetable.php",
            "../inc/_imgupload.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
